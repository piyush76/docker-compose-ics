# Development Environment Configuration
# Copy this file to .env and update with your actual values

# Azure Container Registry Configuration
# Set these values in your actual .env file (not committed to git)
ACR_USERNAME=your-acr-username
ACR_PASSWORD=your-acr-password
ACR_REGISTRY=incora.azurecr.io

# Database Configuration - External Oracle Database
TCMIS_DB_SECRET=devpassword123
# Note: ORACLE_ROOT_PASSWORD not needed for external database

# Development specific settings
ENVIRONMENT=development
LOG_LEVEL=DEBUG

# Optional: Override image tags for development
ICS_SERVICE_IMAGE_TAG=latest
ORACLE_IMAGE_TAG=21.3.0-xe

# External database connection (for external tools)
DEV_DB_HOST=infdev-ora01a.tcmis.com
DEV_DB_PORT=1521
DEV_DB_SERVICE=ICSDEV
DEV_DB_USERNAME=tcm_ops

# Azure AD Configuration (Development)
AZURE_AD_TENANT_ID=your-dev-tenant-id
AZURE_AD_CLIENT_SECRET=your-dev-client-secret

# Development URLs
DEV_BASE_URL=http://localhost:9090
DEV_MANAGEMENT_URL=http://localhost:9091

# Monitoring Configuration
GRAFANA_ADMIN_USER=admin
GRAFANA_ADMIN_PASSWORD=admin123
PROMETHEUS_RETENTION=200h
LOKI_RETENTION=168h

# Alerting Configuration
SLACK_WEBHOOK_URL=https://hooks.slack.com/services/YOUR/SLACK/WEBHOOK
SMTP_HOST=localhost:587
SMTP_FROM=alertmanager@yourcompany.com
SMTP_USERNAME=alerts@yourcompany.com
SMTP_PASSWORD=your-smtp-password
ALERT_EMAIL_TO=admin@yourcompany.com
PAGERDUTY_INTEGRATION_KEY=your-pagerduty-key

# Tracing Configuration
JAEGER_AGENT_HOST=jaeger-dev
JAEGER_AGENT_PORT=6831
